<h3>Edit Employee </h3>
{{#if msg}}
<div class="alert">{{msg}}</div>
{{/if}}
{{#each rows}}
<form method="post" action="/editUser/{{this.ID}}">
<div class="form-group">
    <label for="Ename">EName</label>
    <input type="text" name="Ename" value="{{this.ENAME}}"/>
</div>
<div class="form-group">
    <label for="age">Age</label>
    <input type="number" name="age" value="{{this.AGE}}"/>
</div>
<div class="form-group">
    <label for="salary">Salary</label>
    <input type="number" name="salary" value="{{this.SALARY}}"/>
</div>
<div class="form-group">
    <label for="city">City</label>
    <input type="text" name="city" value="{{this.CITY}}"/>
</div>
<div class="form-group">
    <button type="submit">Save</button>
     <a href="/">Back</a>
</div>

</form>
{{/each}}
